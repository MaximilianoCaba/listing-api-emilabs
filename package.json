{
  "name": "listingapi",
  "version": "1.0.0",
  "description": "Welcome to our technical challenge! We're excited to have you here and hope you're ready to showcase your skills.",
  "main": "index.js",
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 7.10.0"
  },
  "scripts": {
    "test": "jest",
    "test:cov": "jest --coverage",
    "dev": "nodemon",
    "start": "ts-node index.ts",
    "generateToken": "ts-node src/script/generateToken.ts",
    "migrate": "npx sequelize-cli db:migrate",
    "unmigrate": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "unseed": "npx sequelize-cli db:seed:undo:all",
    "lint": "eslint . --ext .ts"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/sqlite3": "^3.1.8",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "~8.8",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "sequelize-cli": "^6.6.0",
    "sqlite3": "^5.1.6",
    "ts-jest": "^29.1.0"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/express-jwt": "^7.4.2",
    "@types/jsonwebtoken": "^9.0.1",
    "csv-parser": "^3.0.0",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.30.0",
    "stream": "^0.0.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4",
    "typescript-rest": "^3.0.4"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
